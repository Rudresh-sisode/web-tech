{
  "name": "checkincheckout",
  "version": "1.0.0",
  "description": "In-house project for check in and check out",
  "main": "index.js",
  "scripts": {
    "test": "vitest --run --reporter verbose",
    "dev": "nodemon dist/src/server.js",
    "start": "nodemon --exec npx ts-node src/server.ts",
    "build": "tsc --project . && cpy ./src/abstractions/seeds/*.json ./dist/abstractions/seeds && cpy ./src/abstractions/seeds/services/*.png ./dist/abstractions/seeds/services"
  },
  "keywords": [
    "node",
    "typescript",
    "postgresql",
    "sequelize"
  ],
  "author": "rudresh sisodiya",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pgtools": "^1.0.0",
    "pug": "^3.0.2",
    "sequelize": "^6.31.0",
    "sharp": "^0.32.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.16.0",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "@types/pug": "^2.0.6",
    "@types/sequelize": "^4.28.14",
    "cpy-cli": "^4.2.0",
    "jest": "^29.5.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vitest": "^0.30.1"
  }
}
